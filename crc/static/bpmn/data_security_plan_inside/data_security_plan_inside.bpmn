              <camunda:property id="enum_type" value="checkbox" />
              <camunda:property id="group" value="hipaa" />
              <camunda:property id="repeat" value="hipaa" />
              <camunda:property id="hide_expression" value="model.HIPAA_Ids === &#39;HIPAA_Ids0&#39;" />
